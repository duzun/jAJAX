{
  "name": "jajax.js",
  "version": "1.1.1",
  "description": "jQuery-like AJAX method for multiple environments",
  "author": {
    "name": "Dumitru Uzun",
    "email": "contact@duzun.me",
    "url": "http://DUzun.Me/"
  },
  "keywords": [
    "AJAX",
    "HTTP",
    "chrome-extension",
    "safari-extension",
    "firefox-extension",
    "addon",
    "javascript",
    "jQuery"
  ],
  "main": "./jajax.js",
  "files": [
    "dist/",
    "LICENSE",
    "README.MD"
  ],
  "directories": {
    "tools": "./tools",
    "doc": "./docs",
    "distribution": "./dist"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/duzun/jAJAX.git"
  },
  "bugs": {
    "url": "https://github.com/duzun/jAJAX/issues"
  },
  "license": [
    {
      "type": "MIT",
      "url": "https://github.com/duzun/jAJAX/raw/master/LICENSE"
    }
  ],
  "readmeFilename": "README.md",
  "engines": {
    "node": ">= 0.8.0"
  },
  "scripts": {
    "test": "jasmine-node spec",
    "bump-ver": "node ./tools/bump-ver.js",
    "verup": "node ./node_modules/verup",
    "build": "node ./tools/build.js",
    "doc": "docco jajax.js"
  },
  "devDependencies": {
    "jasmine-node": "1.14.x",
    "verup": "1.x.x",
    "docco": "0.6.x"
  },
  "verup": {
    "files": [
      "jajax.js",
      "bower.json",
      "README.md"
    ],
    "regs": [
      "^((?:\\$|(\\s*\\*\\s*@)|(\\s*(?:var|,)?\\s+))version[\\s\\:='\"]+)([0-9]+(?:\\.[0-9]+){2,2})",
      "^(\\s?\\*.*v)([0-9]+(?:\\.[0-9]+){2,2})"
    ]
  }
}
